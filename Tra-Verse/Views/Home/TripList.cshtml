
<h1>Choose Your Vacation :)</h1>
@TempData["LoggedIn"]
@*Search function!!!!!!!!!!!!*@
@for (int i = 0; i <= 10; i++)
{
    <span>PLANET PICTURE</span>
    <p>Planet Name: @ViewBag.NASAInfo[i]["pl_name"]</p>
    <p>Distance from Earth: @ViewBag.NASAInfo[i]["st_dist"] parsecs</p>
    <p>Business Price: @ViewBag.YelpInfo["businesses"][i]["price"]</p>
    <p>Review count: @ViewBag.YelpInfo["businesses"][i]["review_count"]</p>
    <p>Rating: @ViewBag.YelpInfo["businesses"][i]["rating"]</p>
    <a href="Home/PrivateAccomodations?index=i">Choose this Destination</a>
    <br />
}